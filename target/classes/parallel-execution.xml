<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="ParallelTestSuite">
	<listeners>
		<listener class-name="com.pgw.qa.ExtentReports.TestListener" />
	</listeners>
	<test name="Parallel Tests" parallel="classes" thread-count="2">
		<classes>
			<class name="com.pgw.qa.testcases.HomePageTest" />
			<class name="com.pgw.qa.testcases.PaymentProcessPageTest" />
			<class name="com.pgw.qa.testcases.OrderIdPageTest" />
		</classes>
	</test>
</suite> <!-- Suite -->
